    <script>      
      function editForm(ls,index) {
         var ed = index.toString();
         ed="#"+"id"+ed;
         document.querySelector(ed).setAttribute("data-toggle", "modal"); 
         document.querySelector(ed).setAttribute("data-target", "#exampleModal"); 
         document.getElementById("title").value = ls[index].title;
         document.getElementById("price").value = ls[index].price;
         document.getElementById("category").value = ls[index].category;
         document.getElementById("description").value = ls[index].description;
         document.getElementById("myid").value = ls[index]._id;
        }
    </script>
